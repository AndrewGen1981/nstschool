    <script defer>
        // for accordion
        const toggle = document.querySelector(".about-toggle")
        const aboutBox = document.querySelector(".about-box")

        function switchToggle() {
            if (toggle.name === 'toggle-hidden') {
                aboutBox.style.maxHeight = 'none'
                toggle.style.top = `${56+aboutBox.scrollHeight}px`
                toggle.textContent = 'close'
                toggle.name = 'toggle-shown'
            } else {
                aboutBox.style.maxHeight = '0px'
                toggle.style.top = '56px'
                toggle.textContent = 'About School'
                toggle.name = 'toggle-hidden'
            }
        }

        toggle.addEventListener('click', (e)=>{
            switchToggle()
        })
        aboutBox.addEventListener('click', (e)=>{
            switchToggle()
        })

        window.addEventListener('resize', (e)=>{
            if (toggle.name === 'toggle-shown') {
                toggle.style.top = `${56+aboutBox.scrollHeight}px`
            }
        })

    </script>

